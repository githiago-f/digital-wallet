{
    "editor.tabSize": 4,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "[typescriptreact]": {
        "files.eol": "\n",
        "files.encoding": "utf8",
        "files.insertFinalNewline": true,
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        }
    }
}
